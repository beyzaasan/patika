public class Fibonacci {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int[] arr = new int[9];
		arr[0]=0;
		arr[1]=1;
		for(int i=0;i<=6;i++) {
			arr[i+2]=arr[i]+arr[i+1];
		}
		System.out.print("9 elemanlÄ± fibonacci serisi: ");
		for(int k=0;k<9;k++) {
			System.out.print(arr[k]+" ");
		}
	}

}
